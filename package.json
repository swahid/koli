{
  "name": "koli",
  "version": "0.0.1",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "16.11.1"
  },
  "scripts": {
    "release": "npx react-native run-android --variant=release",
    "android": "npx react-native run-android --variant=staging",
    "ios": "npx react-native run-ios --scheme 'KOLI QA'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . || echo DONE",
    "format": "npx prettier-standard App/**/*.js || echo DONE",
    "secure": "npm audit fix",
    "audit": "npx retire || echo DONE",
    "stagingbuild": "cd android && SET ENVFILE=.env.staging && gradlew assemblekoliQAStaging",
    "build:ios": "react-native bundle --minify --entry-file index.js --platform ios --dev false --bundle-output ./ios/main.jsbundle --assets-dest ./ios",
    "build:android": "npx react-native bundle --minify --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "react-native clean-project-auto"
  },
