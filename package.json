    "react-test-renderer": "17.0.2",
    "retire": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation|react-native-flash-message|react-native-read-more-text|react-native-simple-toast|tipsi-stripe)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./jest.setup.js"
    ],
    "moduleNameMapper": {
      "countries.json": "<rootDir>/__mocks__/countries.json"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "svg"
    ]
  }
