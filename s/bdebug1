npx jetify && npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleStaging && cd .. && ls -lah android/app/build/outputs/apk/staging/staging*apk && open android/app/build/outputs/apk/staging/
